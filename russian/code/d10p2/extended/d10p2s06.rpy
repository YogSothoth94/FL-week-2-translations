translate russian d10p2s06_46176c4d:

    # jf "*Intensely agreeing noises*"
    jf "*Усиленно кивает в знак согласия*"

# game/code/d10p2/extended/d10p2s06.rpy:117
translate russian d10p2s06_4e69b159:

